CMD:gendong(playerid, params[])
{
    new yangdigendong, string[512], string2[512];
    if(sscanf(params, "u", yangdigendong)) return SendClientMessage(playerid, COLOR_WHITE, "/carry [playerid] NOTE: Jika ingin letakkan /drag [playerid] lagi.");
    if(GetDistanceBetweenPlayers(playerid, yangdigendong) < 2)
    {
        if(Targetgendong[yangdigendong] == 0 && Yangmenggendong[playerid] == 0)
        {
            Targetgendong[yangdigendong] = 1;
            Yangmenggendong[playerid] = 1;
            format(string, sizeof(string), "Anda digendong oleh {FF0000}%s.",(playerid));
            format(string2, sizeof(string2), "Anda menggendong {FF0000}%s.",(yangdigendong));
            ApplyAnimation(yangdigendong, "FAT", "IDLE_tired", 4.1, 0, 1, 1, 1, 0, 1);
            gendongtimer[yangdigendong] = SetTimerEx("Draggingt", 1000, 1, "dd", playerid,yangdigendong);
        }
        else if(Targetgendong[yangdigendong] == 1 && Yangmenggendong[playerid] == 1)
        {
            Targetgendong[yangdigendong] = 0;
            Yangmenggendong[playerid] = 0;
            Info(playerid, "Anda berhenti menggendong.");
            Info(yangdigendong, "Anda tidak digendong lagi.");
            ClearAnimations(playerid);
            KillTimer(gendongtimer[yangdigendong]);
        }
    }
    else
    {
        Info(playerid, "Anda tidak dekat dengan pemain tersebut.");
    }
    return 1;
}

